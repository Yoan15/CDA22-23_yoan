<Application x:Class="CompteRenduWagon.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CompteRenduWagon"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         
        <ImageBrush x:Key="BtnMoins" ImageSource="/moins.png"/>
        <ImageBrush x:Key="BtnPlus" ImageSource="/plus.png"/>
        <LinearGradientBrush x:Key="BackGroundApp" EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FF95B6B4"/>
            <GradientStop Color="White" Offset="1"/>
        </LinearGradientBrush>
        <SolidColorBrush x:Key="NonEditable" Color="LightGray"/>
        <SolidColorBrush x:Key="BtnBorduresReinitialiser" Color="#FFFFC500"/>
        <SolidColorBrush x:Key="BtnBorduresAnnuler" Color="#FFFF0707"/>
        <SolidColorBrush x:Key="BtnBorduresValider" Color="#FF5DFF00"/>
        <SolidColorBrush x:Key="BtnBorduresImport" Color="#FF2435E4"/>
    </Application.Resources>
</Application>
